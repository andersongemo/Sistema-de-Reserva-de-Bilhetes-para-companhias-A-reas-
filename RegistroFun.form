    private void SalvarFunActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_SalvarFunActionPerformed
        // TODO add your handling code here:
     SalvarFun.addActionListener(new ActionListener() {
     public void actionPerformed(ActionEvent e){
     String nomeFun = txtNomeFun.getText();
     String apelidoFun = txtApelidoFun.getText();
     String nuitFun = txtNuitFun.getText();
     String telefoneFun = txtTelefoneFun.getText();
     String dataNasFun = txtDataNasFun.getText();
     String senhaFun = new String (txtSenhaFun.getPassword());
     
     
      try(Connection conn = conexao.conectar()){
          String sql = "INSERT INTO funcionario (nomeFun, apelidoFun, nuitFun, telefoneFun, dataNasFun, senhaFun) Values (?,?,?,?,?,?)";
       PreparedStatement stmt = conn.prepareStatement(sql);
